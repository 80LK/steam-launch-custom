{
	"compilerOptions": {
		"target": "ES2020",
		"module": "esnext",
		"lib": [
			"es2020",
		],
		"esModuleInterop": true,
		"skipLibCheck": true,
		/* Bundler mode */
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		/* Linting */
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	}
}
